<div class="header-side">
  <div class="header-name">
    <%= link_to current_user.store_name , store_path(current_user.id) , method: :get %> 
  </div>
  
</div>
<div class="store-names">
<% @users.all.where.not(id: current_user.id).each do |user| %>
  <div class="store-name">
    <%= link_to  user.store_name , store_path(user.id)%>
  </div>
  <% end %>
</div>